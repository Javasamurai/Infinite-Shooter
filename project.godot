; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="TopDownShooter"
run/main_scene="res://Nodes/MainMenu.tscn"
boot_splash/image="res://Images/empty_sky.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://Images/Players/Level_1_Player_1.png"

[autoload]

Globals="*res://Scripts/Globals.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=243
window/size/height=405
window/size/always_on_top=true
window/size/test_width=320
window/size/test_height=480
window/handheld/orientation="sensor_portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/per_pixel_transparency/splash=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_environment="res://default_env.tres"
